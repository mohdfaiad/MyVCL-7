  vcl,
  inet,
  {$IFDEF DELPHI7}
  cxWebD7,
  cxScriptEngineD7;
  {$ELSE}
  cxWebD6,
  cxScriptEngineD6;
  {$ENDIF}

contains
  SimpleTabControl in 'SimpleTabControl.pas';

