<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <MainSource>MasterViewColumnsDemo.cpp</MainSource>
    <Config Condition="'$(Config)'==''">Base</Config>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Config)'=='Base' or '$(Base)'!=''">
    <Base>true</Base>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Base)'!=''">
    <DCC_CBuilderOutput>JPHNE</DCC_CBuilderOutput>
    <Defines>NO_STRICT</Defines>
    <ProjectType>CppVCLApplication</ProjectType>
    <BCC_wpar>false</BCC_wpar>
    <IncludePath>$(BDS)\include;$(BDS)\include\dinkumware;$(BDS)\include\vcl;..\Common;..</IncludePath>
    <AllPackageLibs>rtl.lib;vcl.lib;dxComnC11.lib;dxMasterViewC11.lib;bcbsmp.lib;vcldb.lib;dbrtl.lib;bdertl.lib;adortl.lib;;vclx.lib</AllPackageLibs>
    <ILINK_LibraryPath>$(BDS)\lib\release;$(BDS)\lib;$(BDS)\lib\obj;$(BDS)\lib\psdk;..\Common;..</ILINK_LibraryPath>
  </PropertyGroup>
  <ProjectExtensions>
    <Borland.Personality>CPlusPlusBuilder.Personality</Borland.Personality>
    <Borland.ProjectType>CppVCLApplication</Borland.ProjectType>
    <BorlandProject>
      <BorlandProject>
        <CPlusPlusBuilder.Personality>
        </CPlusPlusBuilder.Personality>
      </BorlandProject>
    </BorlandProject>
  </ProjectExtensions>
  <Import Project="$(MSBuildBinPath)\Borland.Cpp.Targets" />
  <ItemGroup>
    <ResFiles Include="MasterViewColumnsDemo.res">
    </ResFiles>
    <CppCompile Include="MasterViewColumnsDemo.cpp"> </CppCompile>
    <CppCompile Include="Unit1.cpp">
      <Form>fmMain</Form>
    </CppCompile>
    <FormResources Include="Unit1.dfm" />
  </ItemGroup>
</Project>